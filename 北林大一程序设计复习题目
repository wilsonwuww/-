1. 1，2，3，4可以组成多少个不含重复数字的三位数
#include<stdio.h>
int main()
{
    int number=0;
    for(int i=1;i<5;i++)
    {
        for(int j=1;j<5;j++)
        {
            for(int k=1;k<5;k++)
            if(i!=j&&j!=k&&i!=k)
            {
                printf("%d%d%d ",i,j,k);
                number++;
            }
        }
    }
    printf("可以组成%d个三位数",number);
    return 0;
}
2.一个数字加上156 100为完全平方数
#include<stdio.h>
#include<math.h>
int main()
{
    for(int number=0;number<1000;number++)
    {
        int x=sqrt(number+100);
        int y=sqrt(number+156);
        if(x*x==number+100&&y*y==number+156)
        {
            printf("%d",number);
            break;
        }
    }
    return 0;
}
