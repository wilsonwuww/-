1. 1，2，3，4可以组成多少个不含重复数字的三位数
#include<stdio.h>
int main()
{
    int number=0;
    for(int i=1;i<5;i++)
    {
        for(int j=1;j<5;j++)
        {
            for(int k=1;k<5;k++)
            if(i!=j&&j!=k&&i!=k)
            {
                printf("%d%d%d ",i,j,k);
                number++;
            }
        }
    }
    printf("可以组成%d个三位数",number);
    return 0;
}
